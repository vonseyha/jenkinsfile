pipeline {
    agent any;
    parameters {
        choice(name: "TEST_CHOICE", choices: ["production", "staging", "development"], description: "Samplemulti-choice parameter")
    }
    stages {
        stage('test choice') {
            steps {
                echo "${TEST_CHOICE}"
                script {
                    def gitbranch = "${TEST_CHOICE}"
                    echo "branch git :" gitbranch 
                }
            }
        }
    }
}
